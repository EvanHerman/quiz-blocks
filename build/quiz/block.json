{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "quizblocks/quiz",
  "version": "0.1.0",
  "title": "Quiz",
  "icon": "forms",
  "description": "Display a quiz on your website.",
  "category": "quiz-blocks",
  "supports": {
    "html": false
  },
  "attributes": {
    "quizID": {
      "type": "integer",
      "default": 0
    },
    "requireLogin": {
      "type": "boolean",
      "default": true
    },
    "useRankings": {
      "type": "boolean",
      "default": true
    },
    "multipleSubmissions": {
      "type": "boolean",
      "default": true
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showAnswers": {
      "type": "boolean",
      "default": true
    },
    "showResults": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "quiz-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}