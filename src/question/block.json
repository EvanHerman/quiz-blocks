{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "quizblocks/multiple-choice-question",
	"version": "0.1.0",
	"title": "Multiple Choice Question",
	"icon": "forms",
	"description": "Multiple choice question.",
	"supports": {
		"html": false
	},
	"attributes": {
		"question": {
			"type": "string",
			"default": ""
		},
		"answerCount": {
			"type": "integer",
			"default": 4
		},
		"answers": {
			"type": "array",
			"source": "children",
			"selector": ".wp-block-quizblocks-answer",
			"default": []
		}
	},
	"textdomain": "quiz-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
